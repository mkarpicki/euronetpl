{
    "version": "1.2",
    "name": "eBankomaty",
    "description": "Aplikacja umożliwia wyszukanie bankomatów sieci Euronet na terenie podanego lub znalezionego za pomocą nadajnika GPS miasta na terenie Polski.",
    "launch_path": "/index.html",

    "developer": {
        "name": "Mariusz Karpicki",
        "url": "http://www.linkedin.com/in/mkarpicki"
    },

    "permissions": {
        "systemXHR": {
            "description": "Połączenie się z serwerem w celu pobrania listy bankomatów"
        },
        "geolocation": {
            "description": "Ustalenie pozycji użytkownika w celu wyszukania bankomatów w pobliżu"
        }
    },

    "icons": {
        "48": "/img/app-icons/48x48.png",
        "128": "/img/app-icons/128x128.png"
    },

    "default_locale": "pl",

    "fullscreen": "true"

}